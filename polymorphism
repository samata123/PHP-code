interface machine
{
    public function caltask();
}
class circle implements machine
{
    public function caltask()
    {
        echo "hiii";
    }
}
class rectangle implements machine
{
   // public $name;
    public function caltask()
    {
        echo "heloo";
       // echo $name;
    }
}

$cirlce=new circle();
$rectangle =new rectangle();
echo $cirlce->caltask();
echo $rectangle ->caltask("apple");

